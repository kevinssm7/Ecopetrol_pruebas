                <div id="DivLoadingPage"></div>
                <div class="modal fade" data-backdrop="static" data-keyboard="false" id="SpinnerCarga">
                    <img id="imgloader" src="~/Resources/cargando.gif" alt="Loading">
                </div>
            </div>
        </div>


