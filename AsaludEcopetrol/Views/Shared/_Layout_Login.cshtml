        .pagination > .active > a {
            background-color: #333333 !important;
            border-color: white !important;
        }

        #ModalAnalitica iframe {
            width: 100%;
            min-height: 65vh;
        }

        #ModalAnalitica .modal-dialog {
            margin-right: 40%;
