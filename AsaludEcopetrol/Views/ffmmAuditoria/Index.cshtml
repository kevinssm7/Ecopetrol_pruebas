    function calcularGrupoEtareo(age) {

        var gEtareoCalculado = 0;

        if (age >= 60) {
            gEtareoCalculado = 6;
        } else if (age >= 29) {
            gEtareoCalculado = 5;
        } else if (age >= 18) {
            gEtareoCalculado = 4;
        } else if (age >= 12) {
            gEtareoCalculado = 3;
        } else if (age >= 6) {
            gEtareoCalculado = 2;
        } else {
            gEtareoCalculado = 1;
        }
        return gEtareoCalculado;
    }
