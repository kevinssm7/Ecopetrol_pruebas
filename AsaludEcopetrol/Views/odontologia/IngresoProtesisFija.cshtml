                $("#tbl3_pert_" + i).prop('disabled', true);
            } else {
                $("#tbl3_ter_" + i).prop('disabled', false);
                $("#tbl3_fun_" + i).prop('disabled', false);
                $("#tbl3_pert_" + i).prop('disabled', false);


                if ($("#tbl3_ter_" + i).val() == 0 || $("#tbl3_fun_" + i).val() == 0 || $("#tbl3_pert_" + i).val() == 0) {

                    $("#alerta").val("SI");

                } else {
                    $("#alerta").val("NO");
                }
            }
        }
