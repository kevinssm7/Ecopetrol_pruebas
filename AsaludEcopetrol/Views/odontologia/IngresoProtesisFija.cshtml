<script src="~/Scripts/validate.js"></script>
<script>

    var dientesUnoArray = [];
    var dientesDosArray = [];

    $(document).ready(function () {
        $('.select2').select2({
            width: '100%'
        });

        $(".datepickerr").kendoDatePicker({
            format: "MM/dd/yyyy",
        });
        $(".k-input").attr('readonly', true);

        var mivalor = '@ViewBag.alerta'
        $("#alerta").val(mivalor);


        $("#optpriemeraRepetida").change(function () {


            var mivalor = $("#optpriemeraRepetida").val()
              var mitexto = $("#optpriemeraRepetida option:selected").text()

              if (mivalor == "NO") {
                 document.getElementById('pnlrepetida').style.display = 'block';
              } else {
                  document.getElementById('pnlrepetida').style.display = 'none';
              }
          })

        traerDientesUno();
        traerDientesDos();

        var lista = '@Model.detalle_dientes_uno';
        var listados = '@Model.detalle_dientes_dos';
        if (lista != "" && listados != "" )
        {

            var listado = lista.split(',');
            var count = 0;
            for(var l = 0; l < @Dientes_LineaUno.Length; l++)
            {
                $("#tbl2_id_diente_" + l).val(listado[count]);
                count += 1;
                $("#tbl2_PA_" + l).val(listado[count]);

                count += 1;
                $("#tbl2_ter_" + l).val(listado[count]);
                count += 1;
                $("#tbl2_fun_" + l).val(listado[count]);
                count += 1;
                $("#tbl2_pert_" + l).val(listado[count]);
                count += 1;
            }

            listado = listados.split(',');
            count = 0;
            for(var l = 0; l < @Dientes_LineaDos.Length; l++)
            {
                $("#tbl3_id_diente_" + l).val(listado[count]);
                count += 1;
                $("#tbl3_PA_" + l).val(listado[count]);
                count += 1;
                $("#tbl3_ter_" + l).val(listado[count]);
                count += 1;
                $("#tbl3_fun_" + l).val(listado[count]);
                count += 1;
                $("#tbl3_pert_" + l).val(listado[count]);
                count += 1;
            }
        }

        //obtenervaloresDientesUno();
        //obtenervaloresDientesDos();

        $("form[name='frm-gasto']").validate({
            lang: 'Es',
            rules: {
                Obs: "required",
                id_regional: { required: true },
            },
            messages: {
                file: 'No se ha seleccionado ningun archivo'
            },
            submitHandler: function (form)
            {
                LoadingShow();
                form.submit();
            }
        });

        jQuery.extend(jQuery.validator.messages, {
            required: "Este Campo es Obligatorio *",
        });

        $("#k-checkbox-label").text("Seleccionar todo");
    })

    function traerDientesUno() {

        $("#dientesLineaUno").kendoDropDownTree({
            checkboxes: true,
            placeholder: "Seleccionar...",
            filter: "startswith",
            dataSource: new kendo.data.HierarchicalDataSource({
                transport: {
                    read: {
                        url: '@Url.Action("traerDientes", "odontologia")',
                        dataType: "json",
                        data: {
                            tipo: 1
                        }
                    }
                },
                schema: {
                    model: {
                        id: "value",
                    }
                }
            }),
            placeholder: "Seleccionar...",
            checkboxes: true,
            checkAll: true,
            autoClose: false,
        });
    }

    function traerDientesDos() {

        $("#dientesLineaDos").kendoDropDownTree({
            checkboxes: true,
            placeholder: "Seleccionar...",
            filter: "startswith",
            dataSource: new kendo.data.HierarchicalDataSource({
                transport: {
                    read: {
                        url: '@Url.Action("traerDientes", "odontologia")',
                        dataType: "json",
                        data: {
                            tipo: 2
                        }
                    }
                },
                schema: {
                    model: {
                        id: "value",
                    }
                }
            }),
            placeholder: "Seleccionar...",
            checkboxes: true,
            checkAll: true,
            autoClose: false,
        });
    }

    function LlenarNuevosArrays() {

        debugger
        var dientes1 = $("#dientesLineaUno").data("kendoDropDownTree");
        var listaUno = dientes1.value();

        var dientes2 = $("#dientesLineaDos").data("kendoDropDownTree");
        var listaDos = dientes2.value();

        if (listaUno.length > 0 || listaDos.length > 0) {

            $("#dientes_uno").val(listaUno);
            $("#dientes_dos").val(listaDos);
            $("#divAuditoria").empty();
            $("#divAuditoria").load("@Url.Action("AuditoriaDientesFija", "odontologia")?dientesUno=" + listaUno + "&dientesDos=" + listaDos);


            document.getElementById("divAuditoria").style.display = "block";
        }

        else {
            alert("Seleccione los dientes a evaluar de la parte superior o inferior");
            return false;
        }
    }



    function obtenervaloresDientesUno() {

        debugger
        var dienteUno = $("#dientes_uno").val();
        dientesUnoArray = dienteUno.split(",");

        var parametro = new Array(dientesUnoArray.length);
        for (var i = 0; i < parametro.length; i++) {
            parametro[i] = new Array(4);
            parametro[i][0] = $("#tbl2_id_diente_" + i).val();
            parametro[i][1] = $("#tbl2_PA_" + i).val();
            parametro[i][2] = $("#tbl2_ter_" + i).val();
            parametro[i][3] = $("#tbl2_fun_" + i).val();
            parametro[i][4] = $("#tbl2_pert_" + i).val();

            if (parametro[i][1] == 0) {
                $("#tbl2_ter_" + i).prop('disabled', true);
                $("#tbl2_fun_" + i).prop('disabled', true);
                $("#tbl2_pert_" + i).prop('disabled', true);
            }
            else {
                $("#tbl2_ter_" + i).prop('disabled', false);
                $("#tbl2_fun_" + i).prop('disabled', false);
                $("#tbl2_pert_" + i).prop('disabled', false);

                if ($("#tbl2_ter_" + i).val() == 0 || $("#tbl2_fun_" + i).val() == 0 || $("#tbl2_pert_" + i).val() == 0) {

                    $("#alerta").val("SI");

                } else {
                    $("#alerta").val("NO");
                }
            }
        }

        $("#detalle_dientes_uno").val(parametro);
    }

    function obtenervaloresDientesDos() {
        debugger

        var dienteDos = $("#dientes_dos").val();
        dientesDosArray = dienteDos.split(",");

        var parametro = new Array(dientesDosArray.length);

        for (var i = 0; i < parametro.length; i++) {
            parametro[i] = new Array(4);
            parametro[i][0] = $("#tbl3_id_diente_" + i).val();
            parametro[i][1] = $("#tbl3_PA_" + i).val();
            parametro[i][2] = $("#tbl3_ter_" + i).val();
            parametro[i][3] = $("#tbl3_fun_" + i).val();
