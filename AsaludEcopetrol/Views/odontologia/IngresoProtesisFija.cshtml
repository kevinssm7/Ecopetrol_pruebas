    function validarSelects() {
        debugger

        var dientes1 = $("#dientesLineaUno").data("kendoDropDownTree");
        var listaUno = dientes1.value();

        var dientes2 = $("#dientesLineaDos").data("kendoDropDownTree");
        var listaDos = dientes2.value();

        if (listaUno.length > 0 || listaDos.length > 0) {

            var selects = document.querySelectorAll("select[required]");

            for (var i = 0; i < selects.length; i++) {
                var select = selects[i];

                // Verificar si el select y su elemento padre están visibles
                if (select.style.display !== "none" &&
                    window.getComputedStyle(select.parentElement).getPropertyValue("display") !== "none" &&
                    (select.value == "" || select.value == null)) {
                    alert("Los campos de parametros a evaluar son obligatorios.");
                    event.preventDefault(); // Evita que el formulario se envíe si falta una selección
                    return false;
                }
            }

            GuardarDatos();
        }
        else {
            alert("Seleccione los dientes a evaluar de la parte superior o inferior");
            return false;
        }
    }


