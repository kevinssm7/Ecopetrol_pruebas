@model ECOPETROL_COMMON.ENTIDADES.inventario_altoCosto_gestiones

@{
    ViewBag.Title = "GESTIÓN INVENTARIO";
    Layout = "~/Views/Shared/_LayoutECOPETROL.cshtml";
    var rta = ViewBag.rta;
    var mensaje = ViewBag.msg;
    var cerrado = ViewBag.cerrado;
    var conteoLista = ViewBag.conteoGestionadas;
}

<div class="container">
    <input type="hidden" id="id_detalle" name="id_detalle" value="@ViewBag.idDetalle" />

    <ul class="nav nav-pills col-md-4" style="font-size: 10px">
        <li class="active">
            <a class="text-secondary_asalud" href="#1a" id="tab1" data-toggle="tab">INGRESO GESTIÓN</a>
        </li>
        @if (conteoLista > 0)
        {
            <li>
                <a class="text-secondary_asalud" href="#2a" id="tab2" data-toggle="tab">CONTROL GESTIONES</a>
            </li>
        }
    </ul>
    @if (ViewBag.conteoGestionadas > 0)
    {
        <div>
            <a class="button_Asalud_descargas" onclick="MostrarReporteGestionadas(@ViewBag.idDetalle);">
                <i class="glyphicon glyphicon-save-file"></i>&nbsp; DESCARGAR REPORTE
            </a>
        </div>
    }

    <div class="tab-content">
        <div class="tab-pane active row" id="1a">
            <br />
            <div class="tab-content form-group" id="mensajeIngreso">
                @if (rta == 2)
                {
                    <div class="form-group">
                        <div class="alert alert-danger" role="alert">
                            <strong>Error al cargar! </strong>@mensaje
                        </div>
                    </div>
                }
                else if (rta == 1)
                {
                    <div class="form-group">
                        <div class="alert alert-success" role="alert">
                            <strong>Éxito! </strong>@mensaje
                        </div>
                    </div>
                }
            </div>

            <div class="panel panel-default">
                <div class="panel-heading">
                    <div class="panel-body">
                        <form id="formulario" runat="Server" enctype="multipart/form-data" method="post" action="GestionInventario">
                            <br /><br />
                            <div class="row">
                                <input type="hidden" id="idGestion" name="idGestion" value="@ViewBag.idGestion" />
                                <input type="hidden" id="id_inventario" name="id_inventario" value="@ViewBag.idInventario" />
                                <input type="hidden" id="id_detalle" name="id_detalle" value="@ViewBag.idDetalle" />


                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Fecha de diagnóstico del cáncer reportado:</label>
                                    <br /><br />
                                    <br /><br />
                                    <input class="datepickerr form-control" id="fecha_diagnostico" style="width:100%;" name="fecha_diagnostico" value="@Model.fecha_diagnostico" required />

                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Fecha de la nota de remisión o interconsulta del médico o institución general hacia la institución o médico que hizo el diagnóstico:</label>
                                    <input id="fecha_remision" name="fecha_remision" class="datepickerr form-control" style="width:98%;" value="@Model.fecha_remision" required />
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Fecha de ingreso a la institución que realizó el diagnóstico luego de la remisión o interconsulta:</label>
                                    <br />
                                    <br />
                                    <br />
                                    <input id="fecha_ingreso" name="fecha_ingreso" class="datepickerr form-control" style="width:100%;" value="@Model.fecha_ingreso" required />
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Tipo de estudio con el que se realizó el diagnóstico de cáncer:</label>
                                    <br />
                                    <br />
                                    <br />
                                    <br />
                                    <select id="tipo_estudio" name="tipo_estudio" class="select2 form-control" style="width:100%;" required>
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.tipo_estudio" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                            </div>
                            <br /><br />
                            <div class="row">
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Motivo por el cual el usuario no tuvo diagnóstico por histopatología  (aplica para registros con respuesta igual a 7 en la variable anterior):</label>
                                    <select id="motivo_no_diagnostico_usuario" name="motivo_no_diagnostico_usuario" class="select2 form-control" style="width:100%;" required>
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.motivo_no_diagnostico_usuario" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Fecha de recolección de muestra para estudio histopatológico:</label>
                                    <br />
                                    <br />
                                    <br />
                                    <input id="fecha_recoleccion_muestra" name="fecha_recoleccion_muestra" class="datepickerr form-control" style="width:100%;" value="@Model.fecha_recoleccion_muestra" required />
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Fecha de primero o único informe histopatológico válido de diagnóstico:</label>
                                    <br />
                                    <br />
                                    <input id="fecha_primer_informeHispatologico" name="fecha_primer_informeHispatologico" class="datepickerr form-control" style="width:100%;" value="@Model.fecha_primer_informeHispatologico" required />
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Código válido de habilitación de la IPS donde se realiza la confirmación diagnóstica:</label>
                                    <br />
                                    <br />
                                    <select id="codigo_habillitacion_ips" name="codigo_habillitacion_ips" class="select2 form-control" style="width:100%;" required>
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.codigo_habillitacion_ips" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                            </div>
                            <br /><br />

                            <div class="row">
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Fecha de primera consulta con médico tratante de la enfermedad maligna:</label>
                                    <input id="fecha_consulta_medicoTratante" name="fecha_consulta_medicoTratante" class="datepickerr form-control" style="width:100%;" value="@Model.fecha_consulta_medicoTratante" required />
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Histología del tumor en muestra de biopsia o quirúrgica:</label>
                                    <br />
                                    <br />
                                    <select required id="histologia_tumor" class="select2 form-control" style="width:100%;" name="histologia_tumor">
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.histologia_tumor" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Grado de diferenciación del tumor sólido maligno según la biopsia o informe de primera cirugía:</label>
                                    <select required id="grado_diferenciacion_tumor" class="select2 form-control" style="width:100%;" name="grado_diferenciacion_tumor">
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.grado_diferenciacion_tumor" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Si es tumor sólido, cuál fue la primera estadificación basada en TNM, FIGO, u otras compatibles con esta numeración según tumor:</label>
                                    <select required id="estadificacion_tnm_figo" class="select2 form-control" style="width:100%;" name="estadificacion_tnm_figo">
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.estadificacion_tnm_figo" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                            </div>
                            <br /><br />

                            <div class="row">
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Fecha en que se realizó esta estadificación:</label>
                                    <input class="datepickerr form-control" required id="fecha_estadificacion" style="width:100%;" name="fecha_estadificacion" value="@Model.fecha_estadificacion" />
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Para cáncer de mama, ¿se le realizó a este usuario la prueba HER1 (llamado también receptor 1 del factor de crecimiento epidérmico humano, también llamado erb-B1) antes del inicio del tratamiento?:</label>
                                    <select required id="se_realiza_her1" class="select2 form-control" style="width:100%;" name="se_realiza_her1">
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.se_realiza_her1" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Para cáncer de mama, fecha de realización de la última prueba HER1:</label>
                                    <input class="datepickerr form-control" required id="fecha_realizacion_her1" style="width:100%;" name="fecha_realizacion_her1" value="@Model.fecha_realizacion_her1" />
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Para cáncer de mama, resultado de la única o última prueba HER1:</label>
                                    <select required id="resultado_prueba_her1" class="select2 form-control" style="width:100%;" name="resultado_prueba_her1">
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.resultado_prueba_her1" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                            </div>
                            <br /><br />

                            <div class="row">
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Para cáncer colorrectal, estadificación de Dukes:</label>
                                    <select required id="estadificacion_dukes" class="select2 form-control" style="width:100%;" name="estadificacion_dukes">
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.estadificacion_dukes" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Fecha en que se realizó la estadificación de Dukes:</label>
                                    <input class="datepickerr form-control" required id="fecha_estadificacion_dukes" style="width:100%;" name="fecha_estadificacion_dukes" value="@Model.fecha_estadificacion_dukes" />
                                </div>  
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Estadificación clínica  en linfoma no Hodgkin y linfoma Hodgkin adulto y pediátrico (Ann Arbor-Lugano):</label>
                                    <select required id="estadificacion_hodkin" class="select2 form-control" style="width:100%;" name="estadificacion_hodkin">
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.estadificacion_hodkin" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Para cáncer de próstata, valor de clasificación de la escala Gleason en el momento del diagnóstico:</label>
                                    <select required id="clasificacion_escala_gleason" class="select2 form-control" style="width:100%;" name="clasificacion_escala_gleason">
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.clasificacion_escala_gleason" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                            </div>
                            <br /><br />

                            <div class="row">
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Clasificación de riesgo leucemias o linfomas (para toda la población), y sólidos pediátricos:</label>
                                    <select required id="clasificacion_riesgo_leucemialinfoma" class="select2 form-control" style="width:100%;" name="clasificacion_riesgo_leucemialinfoma">
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.clasificacion_riesgo_leucemialinfoma" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Fecha de clasificación de riesgo:</label>
                                    <input class="datepickerr form-control" required id="fecha_clasificacion_riesgo" style="width:100%;" name="fecha_clasificacion_riesgo" value="@Model.fecha_clasificacion_riesgo" />
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Objetivo (o intención) del tratamiento médico inicial (al diagnóstico):</label>
                                    <select required id="objetivo_tratamiento_inicial" class="select2 form-control" style="width:100%;" name="objetivo_tratamiento_inicial">
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.objetivo_tratamiento_inicial" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Objetivo de la intervención médica durante el periodo de reporte:</label>
                                    <select required id="objetivo_intervencion_periodo_prueba" class="select2 form-control" style="width:100%;" name="objetivo_intervencion_periodo_prueba">
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.objetivo_intervencion_periodo_prueba" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                            </div>
                            <br /><br />

                            <div class="row">
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Tiene antecedente de otro cáncer primario (es decir, tiene o tuvo otro tumor maligno diferente al que está notificando):</label>
                                    <select required id="antecedentes_otro_cancer_primario" class="select2 form-control" style="width:100%;" name="antecedentes_otro_cancer_primario">
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.antecedentes_otro_cancer_primario" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Fecha de diagnóstico del otro cáncer primario:</label>
                                    <input class="datepickerr form-control" required id="fecha_otro_cancer_primario" style="width:100%;" name="fecha_otro_cancer_primario" value="@Model.fecha_otro_cancer_primario" />
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">Tipo (CIE-10)  de ese cáncer antecedente o concurrente:</label>
                                    <select required id="tipo_cie10_otro_cancer" class="select2 form-control" style="width:100%;" name="tipo_cie10_otro_cancer">
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.tipo_cie10_otro_cancer" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">¿Recibió el usuario quimioterapia u otra terapia sistémica (incluye quimioterapia, hormonoterapia, inmunoterapia y terapia dirigida) dentro del periodo de reporte?:</label>
                                    <select required id="usuario_recibio_quimioterapia" class="select2 form-control" style="width:100%;" name="usuario_recibio_quimioterapia">
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.usuario_recibio_quimioterapia" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                            </div>
                            <br /><br />

                            <div class="row">
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">
                                        ¿Cuántas fases de quimioterapia recibió el usuario en este periodo de reporte? (aplica para
                                        hemato linfáticos con los siguientes códigos de clasificación diagnóstica CIE-10; C835 Linfoma no
                                        Hodgkin linfoblástico (difuso), C910 Leucemia linfoblástica aguda, C910 Leucemia mieloide aguda,
                                        C914 Leucemia promielocítica aguda y C915 Leucemia mielomonocítica aguda):
                                    </label>
                                    <select required id="cuantas_quimioterapias_recibioUsuario" class="select2 form-control" style="width:100%;" name="cuantas_quimioterapias_recibioUsuario">
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.cuantas_quimioterapias_recibioUsuario" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">
                                        El usuario recibió en este periodo la fase de quimioterapia denominada Prefase o
                                        Citorreducción inicial (aplica solo para leucemia linfoide aguda y linfoma linfoblástico, puede
                                        haber recibido más de una fase):
                                    </label>
                                    <select required id="usuario_recibio_citorreduccion" class="select2 form-control" style="width:100%;" name="usuario_recibio_citorreduccion">
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.usuario_recibio_citorreduccion" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">
                                        El usuario recibió en este periodo la fase de quimioterapia denominada Inducción (aplica
                                        solo para leucemia linfoide o mieloide aguda y linfoma linfoblástico, puede haber recibido más de
                                        una fase):
                                    </label>
                                    <select required id="usuario_recibio_induccion" class="select2 form-control" style="width:100%;" name="usuario_recibio_induccion">
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.usuario_recibio_induccion" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">
                                        El usuario recibió en este periodo la fase de quimioterapia denominada Intensificación
                                        (aplica solo para leucemia linfoide o mieloide aguda y linfoma linfoblástico, puede haber recibido
                                        más de una fase):
                                    </label>
                                    <select required id="usuario_recibio_intensificacion" class="select2 form-control" style="width:100%;" name="usuario_recibio_intensificacion">
                                        <option value="">--Seleccione--</option>
                                        @foreach (ECOPETROL_COMMON.ENTIDADES.ref_inventario_altoCostoCancer_atc item in ViewBag.atc)
                                        {
                                            <option selected="@Model.usuario_recibio_intensificacion" value="@item.codigo_atc">@item.codigo_atc - @item.descripcion</option>
                                        }
                                    </select>
                                </div>
                            </div>
                            <br /><br />

                            <div class="row">
                                <div class="col-md-3">
                                    <label class="text-secondary_asalud">
                                        El usuario recibió en este periodo la fase de quimioterapia denominada Consolidación (aplica
                                        solo para leucemia linfoide o mieloide aguda y linfoma linfoblástico, puede haber recibido más de
