                                        i++;
                                        <tr>
                                            <td>@i</td>
                                            <td>@item.fecha_diagnostico.Value.ToString("dd/MM/yyyy")</td>
                                            <td>@item.fecha_remision.Value.ToString("dd/MM/yyyy")</td>
                                            <td>@item.fecha_recoleccion_muestra.Value.ToString("dd/MM/yyyy")</td>
                                            <td>@item.fecha_primer_informeHispatologico.Value.ToString("dd/MM/yyyy")</td>
                                            <td>@item.fecha_consulta_medicoTratante.Value.ToString("dd/MM/yyyy")</td>
                                            <td class="text-center">
                                                @*<a href="@Url.Action("GestionInventario","InventarioAltoCosto",new { idGestion = item.id_gestion, idDetalle = item.id_detalle, idInventario = item.id_inventario})">
                                                        <i class="glyphicon glyphicon-eye-close"></i>&nbsp; Validar
                                                    </a>*@

                                                <a class="button_Asalud_Aceptar" href="@Url.Action("GestionInventario","InventarioAltoCosto",new { idGestion = item.id_gestion, idDetalle = item.id_detalle, idInventario = item.id_inventario})">
                                                    <i class="glyphicon glyphicon-eye-close"></i>&nbsp; Validar
                                                </a>
                                            </td>
                                            <td class="text-center">
                                                <a class="button_Asalud_Aceptar" onclick="MostrarArchivos(@item.id_gestion, @item.id_detalle);">
                                                    <i class="glyphicon glyphicon-file"></i>&nbsp; Ver
                                                </a>
                                            </td>
                                        </tr>
                                    }
                                }
                                else
                                {
                                    <tr>
                                        <td colspan="14" style="text-align:center; font-size:14px">SIN REGISTROS</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

