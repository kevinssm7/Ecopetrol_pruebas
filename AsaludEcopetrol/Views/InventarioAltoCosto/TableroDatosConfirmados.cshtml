        var urlVista = '@Url.Action("RepositorioInicialDatosConfirmados")?idRegistro=' + idRegistro + "&tipoCargue=" + tipoCargue + "&estado=" + estado;
        $.ajax({
            cache: false,
            async: true,
            type: "GET",
            url: urlVista,
            data: {},
            success: function (response) {
                $('#resultado').html('');
                $('#resultado').html(response);
            }
        });
    }

