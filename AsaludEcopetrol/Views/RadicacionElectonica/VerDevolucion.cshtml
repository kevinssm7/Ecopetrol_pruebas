
@{
    ViewBag.Title = "VerDevolucion";

}

<div class="panel panel-default">
    <div class="panel-heading">
        <strong class="text-primary_asalud">Devoluciones</strong>
    </div>
    <div class="panel-body">

        <div class="table-responsive">

            <table class="table table-bordered table-condensed table-striped datatable1">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Valor factura</th>
                        <th>Valor Glosa</th>
                        <th>Fecha Devolucion</th>
                        <th>Motivo</th>
                        <th>Observación</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (ECOPETROL_COMMON.ENTIDADES.ManagmentDetalleFacturasDevueltas_fisResult item in ViewBag.List)
                    {
                        <tr>
                            <td>@item.id_devolucion_factura</td>
                            <td>@item.valor_factura</td>
                            <td>@item.ValorGlosa</td>
                            <td>@item.fecha_devolucion.Value.ToString("dd/MM/yyyy")</td>
                            <td>@item.motivo</td>
                            <td>@item.obs</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>

    </div>

</div>



