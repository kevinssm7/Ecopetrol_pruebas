
@{
    ViewBag.Title = "Generar Perfil de Rendimiento";
}

<h2>Generar Perfil de Rendimiento</h2>

@if (ViewBag.ErrorMessage != null)
{
    <div class="alert alert-danger" role="alert">
        @ViewBag.ErrorMessage
    </div>
}

<p>Haz clic en el siguiente botón para generar el perfil de rendimiento:</p>

@using (Html.BeginForm("GenerarPerfil", "PerfilRendimiento", FormMethod.Post))
{
    <button type="submit" class="btn btn-primary">Generar Perfil</button>
}
